
#include <stdio.h>
#include <stdlib.h>

int main() {
    FILE *file;
    char line[100];

    
    file = fopen("employees.txt", "r");
    if (file == NULL) {
        printf("Error opening file!\n");
        return 1;
    }

    printf("Employee Details from file:\n\n");

    
    while (fgets(line, sizeof(line), file) != NULL) {
        printf("%s", line);
    }

    fclose(file); 
    return 0;
}

